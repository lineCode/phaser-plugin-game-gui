{
  "name": "phaser-plugin-game-gui",
  "version": "0.1.8",
  "description": "Inspect and manipulate some common game settings",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "prestart": "open index.html",
    "start": "coffeebar --watch *.coffee example/*.coffee",
    "postinstall": "if [[ -d ./example ]]; then cp -v ./node_modules/dat.gui/build/dat.gui.js ./example; fi;",
    "preversion": "eslint index.js example/index.js",
    "pub": "np --no-cleanup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samme/phaser-plugin-game-gui.git"
  },
  "keywords": [
    "phaser"
  ],
  "author": "samme",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samme/phaser-plugin-game-gui/issues"
  },
  "homepage": "https://github.com/samme/phaser-plugin-game-gui#readme",
  "dependencies": {
    "dat.gui": "github:dataarts/dat.gui",
    "phaser": "^2.6.2"
  },
  "devDependencies": {
    "coffeebar": "^1.0.0"
  }
}
